{
  "name": "trekin",
  "version": "0.5.1",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Futa Hirakoba",
    "url": "https://github.com/korosuke613"
  },
  "description": "A library that connects Trello to kintone",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "browser": "lib/index.browser.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "build": "npm-run-all -l -s clean -p build:*",
    "prerelease": "npm-run-all -p lint test -s build",
    "build:compile": "run-p compile",
    "compile": "run-p -l compile:*",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc",
    "clean": "rimraf lib esm umd",
    "lint": "eslint --max-warnings 0 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "npm run compile:cjs -- --watch",
    "release": "shipjs prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/korosuke613/trekin.git"
  },
  "files": [
    "lib",
    "esm",
    "umd"
  ],
  "keywords": [
    "trello",
    "kintone"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/korosuke613/trekin/issues"
  },
  "homepage": "https://github.com/korosuke613/trekin",
  "dependencies": {
    "@kintone/rest-api-client": "^1.5.0",
    "core-js": "^3.6.5",
    "json5": "^2.1.3",
    "node-fetch": "^2.6.1",
    "source-map-support": "^0.5.19",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@cybozu/eslint-config": "^11.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "eslint": "^7.7.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "shipjs": "^0.21.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
